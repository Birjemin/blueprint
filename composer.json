{
    "name": "birjemin/blueprint",
    "type": "library",
    "description": "Controller Blueprint documentation generator.",
    "keywords": ["blueprint", "laravel", "docs"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Jason Lewis",
            "email": "jason.lewis1991@gmail.com"
        }, {
            "name": "Birjemin Master",
            "email": "birjemin@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "5.1.* || 5.2.* || 5.3.* || 5.4.*",
        "illuminate/filesystem": "5.1.* || 5.2.* || 5.3.* || 5.4.*",
        "doctrine/annotations": "~1.2",
        "phpdocumentor/reflection-docblock": "3.1.*"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "~2.0",
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Birjemin\\Blueprint\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Birjemin\\Blueprint\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2.1-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
